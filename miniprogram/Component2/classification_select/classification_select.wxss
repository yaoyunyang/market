    flex-direction: row;
    align-items: center;
}
.com-sImg{
    position: absolute;
    right: 38rpx;
    top: 38rpx;
    width: 18rpx;
    height: 30rpx;
    transition: all .001s ease;
}
.com-sTxt{
    
    padding:0 20px 0 20px;
    font-size: 14px;
    color: #3E3E3E;
    
}


