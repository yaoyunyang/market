<view class='com-selectBox'>
    <view class='com-sContent' bindtap='selectToggle'>
        <view class='com-sTxt' style="color:{{select_color}}">{{newname}}</view>
        <image src='../icon/Path.png'  class='com-sImg'></image>
    </view>
    <view class='com-sList' wx:if="{{selectShow}}" >
        <view wx:for="{{propArray}}" data-index="{{index}}" wx:key='' class='com-sItem' bindtap='setText' style="color:{{items_color}}">{{item.text}}</view>
    </view>
</view>