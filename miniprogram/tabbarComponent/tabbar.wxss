
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 98rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.363);
}

.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 18rpx;
    height: 100%;
    position: relative;
    padding:5rpx;
}
.tabbar_icon{
    width: 46rpx;
    height: 46rpx;
}
.special-wrapper{
  position: absolute;
  left: 18rpx;
  top: -50rpx;
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
  border-top: 2rpx solid f2f2f3;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
}
.special-wrapper .tabbar_icon{
    width: 100rpx;
    height: 100rpx;
}
.special-text-wrapper{
  width: 46rpx;
  height: 46rpx;
}